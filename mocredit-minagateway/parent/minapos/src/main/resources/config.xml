<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE j8583-config PUBLIC "-//J8583//DTD CONFIG 1.0//EN"
	"http://j8583.sourceforge.net/j8583.dtd">
<j8583-config>

	<!-- These are the ISO headers to be prepended to the message types specified -->
	<header type="1100">600000000000</header>
	<header type="1120">600000000000</header>
	<header type="1260">600000000000</header>
	<header type="1280">600032000000</header>
	<header type="1300">600032000000</header>
	<header type="1200">600032000000</header>
	<header type="1220">600032000000</header>
	<header type="1420">600032000000</header>
	<header type="1240">600032000000</header>
	<header type="1400">600032000000</header>
	<header type="1500">600032000000</header>
	<header type="1320">600032000000</header>
	<header type="1340">600032000000</header>
	<header type="1520">600032000000</header>
	<header type="1820">600032000000</header>
	<header type="1440">600032000000</header>
	<parse type="1100">
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="60" type="NUMERIC" length="6"></field>
		<field num="63" type="ALPHA" length="6"></field>
	</parse>
	<parse type="1120">
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="60" type="NUMERIC" length="12"></field>
	</parse>

	<parse type="1260">
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>

	<parse type="1280">
		<field num="2" type="LLVAR" length="40"></field>
		<field num="4" type="ALPHA" length="12"></field>
		<field num="5" type="LLLVAR" length="500"></field>
		<field num="6" type="LLLVAR" length="500"></field>
		<field num="9" type="ALPHA" length="20"></field>
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="22" type="ALPHA" length="4"></field>
		`
		<field num="25" type="ALPHA" length="2"></field>
		<field num="26" type="ALPHA" length="2"></field>
		<field num="35" type="LLVAR" length="40"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="52" type="ALPHA" length="16"></field>
		<field num="57" type="NUMERIC" length="2"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>

	<!-- 储值卡查询余额 -->
	<parse type="1300">
		<field num="6" type="LLLVAR"></field>
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="22" type="ALPHA" length="4"></field>
		<field num="25" type="ALPHA" length="2"></field>
		<field num="26" type="ALPHA" length="2"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="52" type="ALPHA" length="16"></field>
		<field num="60" type="ALPHA" length="12"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>
	<parse type="1200">
		<field num="4" type="ALPHA" length="12"></field>
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="22" type="NUMERIC" length="4"></field>
		<field num="25" type="NUMERIC" length="2"></field>
		<field num="26" type="NUMERIC" length="2"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="52" type="ALPHA" length="16"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>
	<!-- 二维码收银-查询订单状态 -->
	<parse type="1220">
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="37" type="ALPHA" length="24">11</field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>
	<!-- 积分兑换-查询积分 -->
	<parse type="1240">
		<field num="2" type="ALPHA" length="40"></field>
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="22" type="ALPHA" length="4"></field>
		<field num="25" type="ALPHA" length="2"></field>
		<field num="26" type="ALPHA" length="2"></field>
		<field num="35" type="ALPHA" length="40"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="52" type="ALPHA" length="16"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>
	<!-- 冲正 -->
	<parse type="1320">
		<field num="2" type="ALPHA" length="19"></field>
		<field num="4" type="ALPHA" length="12"></field>
		<field num="6" type="LLLVAR"></field>
		<field num="9" type="ALPHA" length="20"></field>
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="22" type="ALPHA" length="4"></field>
		<field num="25" type="ALPHA" length="2"></field>
		<field num="38" type="ALPHA" length="12"></field>
		<field num="39" type="ALPHA" length="4"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="52" type="ALPHA" length="16"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>
	<parse type="1420">
		<field num="2" type="NUMERIC" length="6"></field>
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>
	<!-- 撤销 -->
	<parse type="1400">
		<field num="2" type="ALPHA" length="19"></field>
		<field num="4" type="ALPHA" length="12"></field>
		<field num="6" type="LLLVAR"></field>
		<field num="9" type="ALPHA" length="20"></field>
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="22" type="ALPHA" length="4"></field>
		<field num="25" type="ALPHA" length="2"></field>
		<field num="35" type="LLVAR" length="40"></field>
		<field num="37" type="ALPHA" length="24"></field>
		<field num="38" type="ALPHA" length="12"></field>
		<field num="39" type="ALPHA" length="2"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="52" type="ALPHA" length="16"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="61" type="NUMERIC" length="16"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>
	<!-- 批结算 -->
	<parse type="1500">
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="48" type="ALPHA" length="46"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="63" type="LLLVAR"></field>
	</parse>
	<parse type="1340">
		<field num="2" type="ALPHA" length="19"></field>
		<field num="4" type="ALPHA" length="12"></field>
		<field num="6" type="LLLVAR"></field>
		<field num="9" type="ALPHA" length="20"></field>
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="22" type="ALPHA" length="4"></field>
		<field num="25" type="ALPHA" length="2"></field>
		<field num="35" type="LLVAR" length="40"></field>
		<field num="37" type="ALPHA" length="24"></field>
		<field num="38" type="ALPHA" length="12"></field>
		<field num="39" type="ALPHA" length="2"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="52" type="ALPHA" length="16"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="61" type="NUMERIC" length="20"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>
	<!-- 批上送 -->
	<parse type="1520">
		<field num="5" type="LLLVAR" length="12"></field>
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="60" type="NUMERIC" length="12"></field>
	</parse>
	
	<parse type="1820">
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="62" type="LLLVAR" ></field>		
	</parse>
	<parse type="1440">
		<field num="11" type="NUMERIC" length="6"></field>
		<field num="41" type="ALPHA" length="16"></field>
		<field num="42" type="ALPHA" length="30"></field>
		<field num="60" type="NUMERIC" length="12"></field>
		<field num="64" type="ALPHA" length="16"></field>
	</parse>
</j8583-config>
