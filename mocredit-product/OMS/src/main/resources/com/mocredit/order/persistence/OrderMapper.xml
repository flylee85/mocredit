<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mocredit.order.persistence.OrderMapper">
	<insert id="save" parameterType="com.mocredit.order.entity.Order">
		insert into
		`t_order`(order_id,activity_id,activity_name,pub_enterprise_id,pub_enterprise_name,
		sup_enterprise_id,sup_enterprise_name,store_id,store_name,`status`,
		card_num,code,integral,start_time,end_time,create_time)
		values(#{t.orderId},#{t.activityId},#{t.activityName},#{t.pubEnterpriseId},#{t.pubEnterpriseName},
		#{t.supEnterpriseId},#{t.supEnterpriseName},#{t.storeId},#{t.storeNam},#{t.status},
		#{t.cardName},#{t.code},#{t.integral},#{t.startTime},#{t.endTime},now())
	</insert>
</mapper>
