<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mocredit.integral.persistence.ActivityMapper">
	<insert id="save" parameterType="com.mocredit.integral.entity.Activity">
		insert into
		`ti_activity`(activity_id,activity_name,activity_status,product_type,amount)
		values(#{t.activityId},#{t.activityName},#{t.activityStatus},#{t.productType},#{t.amount})
	</insert>
</mapper>
