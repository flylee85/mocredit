<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mocredit.integral.persistence.InRequestLogMapper">
	<insert id="save" parameterType="com.mocredit.integral.entity.InRequestLog">
		insert into
		`ti_in_request_log`(ip,interface,request,timestamp,order_id)
		values(#{t.ip},#{t.interfaceUrl},#{t.request},#{t.timestamp},#{t.orderId})
	</insert>
</mapper>
